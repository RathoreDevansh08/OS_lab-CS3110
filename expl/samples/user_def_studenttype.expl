type

    Student
    {
        str name;
        int marks;
    }

endtype

int main()
{
    decl
    
        int temp;
        Student s;
        
    enddecl

    begin
    
        temp = exposcall("Heapset");
        s = exposcall("Alloc", 2);
        s.name = "John";
        s.marks = 89;
        temp = exposcall("Write", -2, s.name);
        temp = exposcall("Write", -2, s.marks);
        temp = exposcall("Free", s);
        return 0; 
        
    end
}
